<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="15" />
    <option name="FONT_FAMILY" value="Fira Code Retina" />
    <option name="USE_LIGATURES" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="IS_WHITESPACES_SHOWN" value="true" />
    <option name="IS_INNER_WHITESPACES_SHOWN" value="false" />
    <option name="IS_TRAILING_WHITESPACES_SHOWN" value="false" />
    <option name="IS_INDENT_GUIDES_SHOWN" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
  </component>
</application>